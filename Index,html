                this.sizeInfo.selectOffsetLeft -= g[3]
            },
            setMenuSize: function(a) {
                this.getSelectPosition();
                var b, c, d, e, f, g, h, i = this.sizeInfo.selectWidth, j = this.sizeInfo.liHeight, k = this.sizeInfo.headerHeight, l = this.sizeInfo.searchHeight, m = this.sizeInfo.actionsHeight, n = this.sizeInfo.doneButtonHeight, o = this.sizeInfo.dividerHeight, p = this.sizeInfo.menuPadding, q = 0;
