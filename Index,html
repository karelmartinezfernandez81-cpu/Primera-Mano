    <script defer>                
        var cbrid;
        var cbrid_key='__cll_brid';
        try { cbrid = localStorage[cbrid_key]; } catch (e) {}
        if (!cbrid) 
        {          
          cbrid = '';
          var possible = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
          for (var i = 0; i < 32; i++)
            cbrid += possible.charAt(Math.floor(Math.random() * possible.length));

          try { localStorage[cbrid_key] = cbrid; } catch (e) {}
        }

        if (cbrid)
        {
            
            $.ajax(
            {
                url: '/actions/setfp',
                type: 'POST',
                data: { 'fp.deviceFP': cbrid },
                beforeSend: function(xhr) { xhr.overrideMimeType("text/plain; charset=x-user-defined"); }
            });
        }                
    </script>
