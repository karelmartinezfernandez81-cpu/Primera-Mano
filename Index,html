    <script>
        function executeIncidence(pedido){
            document.getElementById("pedido").value = pedido;
            document.getElementById("title_remesa").innerHTML = "Causa del fallo Recarga #" + pedido;
            $('#myModalUpdate').modal();
        }

        function executeAbandon(pedido){
            document.getElementById("pedido").value = pedido;
            document.getElementById("title_remesa").innerHTML = "EstÃ¡ seguro de abandonar la recarga #" + pedido;
            document.getElementById("update-abandon").action = "/actions/topup-cubarecarga/abandon-topup/" + pedido;
            $('#myModalUpdateAbandon').modal();
        }

        function executeDelivered(pedido){
            document.getElementById("pedido").value = pedido;
            document.getElementById("title_remesa").innerHTML = "EstÃ¡ seguro de entrega la recarga #" + pedido;
            document.getElementById("update-delivered").action = "/actions/topup-cubarecarga/delivered-topup/" + pedido;
            $('#myModalUpdateDelivered').modal();
        }
        
        function copyToClipboard(element) {
            var $temp = $("<input>");
            $("body").append($temp);
            $temp.val($(element).text()).select();
            document.execCommand("copy");
            $temp.remove();
        }
    </script>
