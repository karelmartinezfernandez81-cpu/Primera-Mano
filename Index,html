    <script>
        // LÃ³gica de Modales Offline: Simplemente abren el modal y settean los valores ocultos
        // El script Tampermonkey puede interceptar los botones de "Submit" de estos formularios si es necesario,
        // o los formularios simplemente no harÃ¡n nada en este entorno offline.
        
        function executeIncidence(pedido){
            document.getElementById("pedido_fallo").value = pedido;
            document.getElementById("title_remesa_fallo").innerHTML = "Causa del fallo Recarga #" + pedido;
            $('#myModalUpdate').modal('show');
        }

        function executeAbandon(pedido){
            document.getElementById("pedido_abandon").value = pedido;
            document.getElementById("title_remesa_abandon").innerHTML = "EstÃ¡ seguro de abandonar la recarga #" + pedido;
            $('#myModalUpdateAbandon').modal('show');
        }

        function executeDelivered(pedido){
            document.getElementById("pedido_delivered").value = pedido;
            document.getElementById("title_remesa_delivered").innerHTML = "EstÃ¡ seguro de entrega la recarga #" + pedido;
            $('#myModalUpdateDelivered').modal('show');
        }
        
        function copyToClipboard(elementSelector) {
            // El script original usa selectores de jQuery/CSS (ej: #phone123)
            var text = $(elementSelector).text();
            if(!text && elementSelector.startsWith('#')) {
                 // Fallback si pasa el ID directamente
                 var el = document.getElementById(elementSelector.substring(1));
                 if(el) text = el.innerText || el.value;
            }

            var $temp = $("<input>");
            $("body").append($temp);
            $temp.val(text).select();
            document.execCommand("copy");
            $temp.remove();
        }

        // Inicializar componentes UI bÃ¡sicos
        $(document).ready(function() {
            $('.dropdown-toggle').dropdown();
            $('.selectpicker').selectpicker();
        });
    </script>
