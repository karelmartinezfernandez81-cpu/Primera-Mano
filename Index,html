        HIDDEN: "hidden" + Hi,
        SHOW: "show" + Hi,
        SHOWN: "shown" + Hi
    }
      , Fi = "fade"
      , Mi = "hide"
      , Wi = "show"
      , Ui = "showing"
      , Bi = {
        animation: "boolean",
        autohide: "boolean",
        delay: "number"
    }
      , qi = {
        animation: !0,
        autohide: !0,
        delay: 500
    }
      , Ki = '[data-dismiss="toast"]'
      , Qi = function() {
        function i(t, e) {
            this._element = t,
            this._config = this._getConfig(e),
            this._timeout = null,
            this._setListeners()
        }
        var t = i.prototype;
        return t.show = function() {
            var t = this;
            p(this._element).trigger(Ri.SHOW),
            this._config.animation && this._element.classList.add(Fi);
            var e = function() {
                t._element.classList.remove(Ui),
                t._element.classList.add(Wi),
                p(t._element).trigger(Ri.SHOWN),
                t._config.autohide && t.hide()
            };
